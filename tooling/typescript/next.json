{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "FoodWagen Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "allowJs": true,
    "noEmit": true,
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ES2022"
    ]
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}


